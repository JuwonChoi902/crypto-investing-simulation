{
  "roots": ["<rootDir>/test/"],
  "testEnvironment": "jsdom",
  "moduleNameMapper": {
      "\\.(css|less|svg)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
  "setupFilesAfterEnv": ["<rootDir>/test/setup.ts"],
  "transform": {
      "^.+\\.tsx?$": "ts-jest"
  }
}